PROCEDURE "jasonhanaapp.db::SP_TopSales" (IN amount INTEGER, OUT result "jasonhanaapp.db::TT_TopSales")
  LANGUAGE SQLSCRIPT
  SQL SECURITY INVOKER
  READS SQL DATA
  AS
BEGIN
  result = select ID, REGION as "region", COUNTRY as "country", AMOUNT as "amount", COMMENTS as "comments" 
             from "JASONHANAAPP_DB_SALES"
             where AMOUNT >= :amount
             order by AMOUNT desc
             ;
END